<h1>THIS IS SHOW</h1>

<p>
  <strong>Username:</strong>
  <%= @user.username %>
</p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

<% if @user.isAdmin %>
    <h1>ADMIN</h1>
<% else %>
    <h1>NOT ADMIN</h1>

<% end %>

<table>
  <tr>
    <td>Username</td>
    <td>Email</td>
  </tr>

  <% if @users %>
      <% @users.each do |user| %>
          <tr>
            <td><%= user.username %></td>
            <td><%= user.email %></td>
          </tr>
      <% end %>
  <% end %>


</table>
