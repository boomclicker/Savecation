<%= render 'creatives/navbar' %>



<header>
  <div class="header-content">
    <div class="header-content-inner">
      <h1>User Profile Page</h1>
      <hr>
      <p>
        <strong>Username:</strong>
        <%= @user.username %>
        <%= @city_id %>
      </p>

      <p>
        <strong>Email:</strong>
        <%= @user.email %>
      </p>

        <% if @user.isAdmin %>

              <h3>ADMIN</h3>
              <table>
                <tr>
                  <td>Username</td>

                  <td>Email</td>
                </tr>
                <% @users.each do |user| %>
                    <tr>
                      <td><%= user.username %></td>
                      <td><%= user.email %></td>
                    </tr>
                <% end %>
          <% end %>


          </table>


    </div>
  </div>
</header>
